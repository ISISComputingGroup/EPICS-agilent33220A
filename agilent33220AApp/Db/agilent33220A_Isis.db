record(mbbi, "$(P)UNITS")
{
    field(DTYP, "stream")
    field(INP, "@agilent33220A.proto getUnits $(PORT)")
    field(SCAN, "1 second")

    field(ZRVL, 0)
    field(ZRST, "VPP")
    field(ONVL, 1)
    field(ONST, "VRMS")
    field(TWVL, 2)
    field(TWST, "DBM") 
}

record(mbbo, "$(P)UNITS:SP")
{
    field(DTYP, "stream")
    field(OUT, "@agilent33220A.proto setUnits $(PORT)")

    field(ZRVL, 0)
    field(ZRST, "VPP")
    field(ONVL, 1)
    field(ONST, "VRMS")
    field(TWVL, 2)
    field(TWST, "DBM") 
}

alias("$(P)UNITS", "$(P)UNITS:SP:RBV")

